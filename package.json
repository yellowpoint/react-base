{
  "name": "react-base",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yellowpoint/react-base.git"
  },
  "dependencies": {
    "customize-cra": "^1.0.0",
    "lib-flexible": "^0.3.2",
    "postcss-pxtorem": "^6.0.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --quiet --fix",
    "*.{css,less}": "stylelint --fix",
    "*.{md,json}": "prettier --write"
  },
  "eslintIgnore": [
    "**/coverage",
    "**/dist",
    "**/build",
    "**/node_modules",
    "!.stylelintrc.js"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "lint-staged": "^15.2.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.1.1",
    "stylelint": "^14.9.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^26.0.0",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
